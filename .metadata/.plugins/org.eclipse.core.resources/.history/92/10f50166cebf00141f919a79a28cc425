
public class Attraction {
	private:
		String name;
		String cityName;
		double rating;
		double distanceFromStart;
		
	public:
		Attraction(){}
		Attraction(String n, String cn, double r, double dfs){
			name = n;
			cityName = cn;
			rating = r;
			distanceFromStart = dfs;
		}
		
		String getName(){
			return name;
		}
		
		String getCity(){
			return cityName;
		}
		
		double getRating(){
			return rating;
		}
		
		double getDistanceFromStart(){
			return distanceFromStart;
		}
}
